<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    tools:context=".plan_history"
    android:layout_height="match_parent">


    <include
        layout="@layout/background"/>

    <include
        android:id="@+id/topnavi"
        layout="@layout/topnavi"/>

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="10dp"
        android:onClick="home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/home" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="10dp"
        android:onClick="heart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView10"
        app:srcCompat="@drawable/heart" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="10dp"
        android:onClick="history"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView13"
        app:srcCompat="@drawable/history" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="10dp"
        android:onClick="profile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/profile" />

    <ImageView
        android:layout_width="302dp"
        android:layout_height="52dp"
        android:layout_marginBottom="72dp"
        android:scaleX="8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/straight" />

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.096">

        <Button
            android:id="@+id/button1"
            android:layout_width="205dp"
            android:layout_height="match_parent"
            android:background="#27caf2"
            android:layout_margin="1dp"
            android:onClick="plan_history"
            android:text="Plan History" />

        <Button
            android:id="@+id/button2"
            android:layout_width="205dp"
            android:layout_height="match_parent"
            android:background="#27caf2"
            android:layout_margin="1dp"
            android:onClick="book_history"
            android:text="Book History" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="380dp"
        android:layout_height="24dp"
        android:orientation="horizontal"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.18">

        <TextView
            android:id="@+id/text_plan"
            android:layout_width="260dp"
            android:layout_height="match_parent"
            android:text="Plan"
            android:textAlignment="center"
            android:textColor="#2196F3"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/text_status"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:text="Price"
            android:textAlignment="center"
            android:textColor="#2196F3"
            android:textSize="20sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="380dp"
        android:layout_height="72dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.25">



        <LinearLayout
            android:layout_width="260dp"
            android:layout_height="match_parent"
            android:background="#3703A9F4"
            android:orientation="vertical">

            <TextView
                android:id="@+id/a"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:text="hotel A"
                android:textAlignment="center"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/b"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:text="food B"
                android:textAlignment="center"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/c"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:text="car C"
                android:textAlignment="center"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="123dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/price_text1"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:textAlignment="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/price_text2"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:textAlignment="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/price_text3"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:textAlignment="center"
                android:text="TextView" />
        </LinearLayout>



    </LinearLayout>



    <LinearLayout
        android:layout_width="380dp"
        android:layout_height="72dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.40">

        <LinearLayout
            android:layout_width="260dp"
            android:layout_height="match_parent"
            android:background="#3703A9F4"
            android:orientation="vertical">

            <TextView
                android:id="@+id/a2"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:text="state location"
                android:textAlignment="center"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/b2"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:text="cost"
                android:textAlignment="center"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/c2"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:text="date"
                android:textAlignment="center"
                android:textSize="16sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/price_text4"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:textAlignment="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/price_text5"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:textAlignment="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/price_text6"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:textAlignment="center"
                android:text="TextView" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="380dp"
        android:layout_height="72dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.549">

        <LinearLayout
            android:layout_width="260dp"
            android:layout_height="match_parent"
            android:background="#3703A9F4"
            android:orientation="vertical">

            <TextView
                android:id="@+id/a3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="state location"
                android:textAlignment="center"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/b3"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:text="cost"
                android:textAlignment="center"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/c3"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:text="date"
                android:textAlignment="center"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/price_text7"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:textAlignment="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/price_text8"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:textAlignment="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/price_text9"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:textAlignment="center"
                android:text="TextView" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="380dp"
        android:layout_height="72dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.70">

        <LinearLayout
            android:layout_width="260dp"
            android:layout_height="match_parent"
            android:background="#3703A9F4"
            android:orientation="vertical">

            <TextView
                android:id="@+id/a4"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:text="state location"
                android:textAlignment="center"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/b4"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:text="cost"
                android:textAlignment="center"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/c4"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:text="date"
                android:textAlignment="center"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/price_text10"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:textAlignment="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/price_text11"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:textAlignment="center"
                android:text="TextView" />

            <TextView
                android:id="@+id/price_text12"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:textAlignment="center"
                android:text="TextView" />
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>