<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".play">

    <include
        android:id="@+id/topnavi"
        layout="@layout/topnavi" />

    <include layout="@layout/background"/>


    <ImageButton
        android:id="@+id/zoo_negara_picture"
        android:layout_width="77dp"
        android:layout_height="77dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="194dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/zoo_negara_77" />

    <ImageButton
        android:id="@+id/heart1"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="210dp"
        android:onClick="sunway_favourite"
        app:layout_constraintStart_toEndOf="@+id/zoo_negara_button"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/love" />

    <Button
        android:id="@+id/zoo_negara_button"
        android:layout_width="167dp"
        android:layout_height="83dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="190dp"
        android:background="@drawable/button_design_3"
        android:onClick="sunway"
        android:text="Zoo Negera"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintStart_toEndOf="@+id/zoo_negara_picture"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/genting_highland_picture"
        android:layout_width="77dp"
        android:layout_height="77dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/zoo_negara_picture"
        app:srcCompat="@drawable/genting_highland_77" />

    <ImageButton
        android:id="@+id/sky_mirror_picture"
        android:layout_width="77dp"
        android:layout_height="77dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/genting_highland_picture"
        app:srcCompat="@drawable/sky_mirror_77" />

    <Button
        android:id="@+id/genting_highland_button"
        android:layout_width="167dp"
        android:layout_height="83dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/button_design_3"
        android:onClick="genting"
        android:text="Genting Highland"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintStart_toEndOf="@+id/genting_highland_picture"
        app:layout_constraintTop_toBottomOf="@+id/zoo_negara_button" />

    <Button
        android:id="@+id/sky_mirror_button"
        android:layout_width="167dp"
        android:layout_height="83dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/button_design_3"
        android:onClick="sky"
        android:text="Sky Mirror"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintStart_toEndOf="@+id/sky_mirror_picture"
        app:layout_constraintTop_toBottomOf="@+id/genting_highland_button" />


    <ImageButton
        android:id="@+id/heart2"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="52dp"
        android:onClick="genting_favourite"
        app:layout_constraintStart_toEndOf="@+id/genting_highland_button"
        app:layout_constraintTop_toBottomOf="@+id/heart1"
        app:srcCompat="@drawable/love" />

    <ImageButton
        android:id="@+id/heart3"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="60dp"
        android:onClick="sky_favourite"
        app:layout_constraintStart_toEndOf="@+id/sky_mirror_button"
        app:layout_constraintTop_toBottomOf="@+id/heart2"
        app:srcCompat="@drawable/love" />


    <ImageView
        android:layout_width="302dp"
        android:layout_height="52dp"
        android:layout_marginBottom="72dp"
        android:scaleX="8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/straight" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginBottom="10dp"
        android:onClick="home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/home" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginBottom="10dp"
        android:onClick="heart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView10"
        app:srcCompat="@drawable/heart" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="10dp"
        android:onClick="history"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView13"
        app:srcCompat="@drawable/history" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="10dp"
        android:onClick="profile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/profile" />

    <ImageView
        android:layout_width="302dp"
        android:layout_height="52dp"
        android:layout_marginBottom="72dp"
        android:scaleX="8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/straight" />


    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="384dp"
        android:layout_height="333dp"
        android:layout_marginBottom="396dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/textbox" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lemon"
        android:text="Theme Park"
        android:textColor="#FFA500"
        android:textSize="40dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15"
        app:layout_constraintEnd_toEndOf="@+id/imageView15"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="@+id/imageView15"
        app:layout_constraintTop_toTopOf="@+id/imageView15"
        app:layout_constraintVertical_bias="0.391" />

</androidx.constraintlayout.widget.ConstraintLayout>