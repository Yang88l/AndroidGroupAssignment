<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".food">

    <include
        android:id="@+id/topnavi"
        layout="@layout/topnavi"/>

    <include
        layout="@layout/background"/>

    <ImageButton
        android:id="@+id/imageButton15"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="336dp"
        android:onClick="horizon_grill_favourite"
        app:layout_constraintStart_toEndOf="@id/horizon_grill"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/love" />

    <ImageButton
        android:id="@+id/image_food2"
        android:layout_width="77dp"
        android:layout_height="77dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="324dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/horizon_grill_77"/>

    <Button
        android:id="@+id/horizon_grill"
        android:layout_width="167dp"
        android:layout_height="83dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="316dp"
        android:onClick="horizon_grill"
        android:text="Horizon Grill"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:background="@drawable/button_design_2"
        app:layout_constraintStart_toEndOf="@id/image_food2"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/cielo_kl"
        android:layout_width="167dp"
        android:layout_height="83dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="188dp"
        android:onClick="cielo_kl"
        android:text="Cielo KL"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:background="@drawable/button_design_2"
        app:layout_constraintStart_toEndOf="@id/image_food"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/image_food"
        android:layout_width="77dp"
        android:layout_height="77dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="196dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cielo_kl_77" />

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="208dp"
        android:onClick="cielo_kl_favourite"
        app:layout_constraintStart_toEndOf="@id/cielo_kl"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/love" />

    <ImageButton
        android:id="@+id/image_food3"
        android:layout_width="77dp"
        android:layout_height="77dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="452dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sky_bar_77" />

    <Button
        android:id="@+id/sky_bar"
        android:layout_width="167dp"
        android:layout_height="83dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="448dp"
        android:onClick="sky_bar"
        android:text="Sky Bar"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:background="@drawable/button_design_2"
        app:layout_constraintStart_toEndOf="@+id/image_food3"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="468dp"
        android:onClick="sky_bar_favourite"
        app:layout_constraintStart_toEndOf="@+id/sky_bar"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/love" />

    <ImageView
        android:layout_width="302dp"
        android:layout_height="52dp"
        android:layout_marginBottom="72dp"
        android:scaleX="8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/straight" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginBottom="10dp"
        android:onClick="home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/home" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginBottom="10dp"
        android:onClick="heart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView10"
        app:srcCompat="@drawable/heart" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="10dp"
        android:onClick="history"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView13"
        app:srcCompat="@drawable/history" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="10dp"
        android:onClick="profile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/profile" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="306dp"
        android:layout_height="258dp"
        android:layout_marginBottom="444dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/textbox" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lemon"
        android:text="Restaurant"
        android:textColor="#FFA500"
        android:textSize="36dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15"
            app:layout_constraintEnd_toEndOf="@+id/imageView15"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="@+id/imageView15"
        app:layout_constraintTop_toTopOf="@+id/imageView15"
        app:layout_constraintVertical_bias="0.391" />


</androidx.constraintlayout.widget.ConstraintLayout>